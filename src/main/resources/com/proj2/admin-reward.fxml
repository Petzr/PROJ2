      </TableView>
      <Button layoutX="81.0" layoutY="118.0" mnemonicParsing="false" onAction="#addReward" prefWidth="138.0" text="Add reward" />
      <Button layoutY="1.0" mnemonicParsing="false" onAction="#backToDashboard" text="&lt;">
