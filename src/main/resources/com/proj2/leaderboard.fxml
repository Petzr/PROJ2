      <Label layoutX="47.0" layoutY="43.0" text="Leaderboard">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <TableView fx:id="leaderboardTable" layoutY="101.0" prefHeight="450.0" prefWidth="300.0">
        <columns>
          <TableColumn prefWidth="75.0" text="User" />
          <TableColumn prefWidth="75.0" text="Points" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
